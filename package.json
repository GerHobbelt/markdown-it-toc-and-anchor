{
  "name": "@gerhobbelt/markdown-it-toc-and-anchor",
  "version": "4.2.0-2",
  "description": "markdown-it plugin to add toc and anchor links in headings",
  "keywords": [
    "markdown-it",
    "markdown-it-plugin",
    "markdown",
    "toc",
    "anchor"
  ],
  "author": "Maxime Thirouin",
  "license": "MIT",
  "repository": "https://github.com/medfreeman/markdown-it-toc-and-anchor.git",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "!**/__tests__"
  ],
  "dependencies": {
    "clone": "^2.1.2",
    "uslug": "^1.0.4"
  },
  "devDependencies": {
    "ava": "2.4.0",
    "@gerhobbelt/babel-cli": "7.2.1-26",
    "@gerhobbelt/babel-plugin-proposal-object-rest-spread": "7.2.1-26",
    "@gerhobbelt/babel-core": "7.2.1-26",
    "@gerhobbelt/babel-eslint": "9.0.0-3",
    "@gerhobbelt/babel-preset-es2015": "7.2.1-26",
    "@gerhobbelt/babel-preset-stage-2": "7.2.1-26",
    "coveralls": "3.0.7",
    "eslint": "6.6.0",
    "eslint-config-i-am-meticulous": "11.0.0",
    "eslint-plugin-import": "2.18.2",
    "@gerhobbelt/markdown-it": "10.0.0-30",
    "nyc": "14.1.1",
    "standard-version": "7.0.0"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --fix .",
    "tests": "ava",
    "pretest": "npm -s run lint",
    "test": "npm -s run tests",
    "test-with-coverage": "nyc npm run test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel src --ignore \"src/__tests__/**/*.js\" --out-dir dist --delete-dir-on-start",
    "release-orig": "standard-version; git push --follow-tags",
    "pub": "npm publish --access public"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-i-am-meticulous"
    ]
  },
  "ava": {
    "files": [
      "**/__tests__/*.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "exclude": [
      "**/__tests__/**"
    ],
    "include": [
      "src/**"
    ]
  }
}
